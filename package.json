{
    "name": "@kdesigner/gen",
    "version": "0.0.0",
    "description": "A unified CLI tool for generating projects using Rust and napi-rs",
    "main": "dist/index.cjs",
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "require": "./dist/index.cjs",
            "types": "./dist/index.d.ts"
        }
    },
    "napi": {
        "name": "generator",
        "triples": {
            "defaults": true,
            "additional": [
                "aarch64-apple-darwin",
                "x86_64-pc-windows-msvc",
                "x86_64-unknown-linux-gnu",
                "aarch64-unknown-linux-gnu"
            ]
        }
    },
    "scripts": {
        "build": "npm run build:dual",
        "build:debug": "napi build --platform dist",
        "build:dual": "npm run build:cjs && npm run build:esm",
        "build:cjs": "napi build --platform --release --js cjs dist && mv dist/index.js dist/index.cjs",
        "build:esm": "napi build --platform --release --js esm dist",
        "prepublishOnly": "napi prepublish -t npm",
        "test": "npm run build && node examples/test-napi.js",
        "clean": "cargo clean && (if exist dist rmdir /s /q dist)",
        "dev": "npm run build:debug && npm test"
    },
    "files": [
        "dist/",
        "README.md"
    ],
    "devDependencies": {
        "@napi-rs/cli": "^2.18.0"
    },
    "keywords": [
        "rust",
        "generator",
        "template",
        "napi",
        "cli",
        "vue",
        "react",
        "java"
    ],
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/yourusername/project-generator.git"
    },
    "optionalDependencies": {
        "@kdesigner/gen-win32-x64-msvc": "0.0.0",
        "@kdesigner/gen-darwin-x64": "0.0.0",
        "@kdesigner/gen-linux-x64-gnu": "0.0.0",
        "@kdesigner/gen-darwin-arm64": "0.0.0",
        "@kdesigner/gen-linux-arm64-gnu": "0.0.0"
    }
}