{
  "name": "vue-basic",
  "version": "1.0.0",
  "description": "Basic Vue 3 project with Vite and TypeScript support",
  "author": "Your Team",
  "project_type": "vue",
  "variables": [
    {
      "name": "project_name",
      "description": "Project name",
      "required": true,
      "type": "string"
    },
    {
      "name": "description", 
      "description": "Project description",
      "required": false,
      "type": "string",
      "default": "A Vue.js project"
    },
    {
      "name": "author",
      "description": "Author name",
      "required": false,
      "type": "string"
    },
    {
      "name": "use_typescript",
      "description": "Use TypeScript",
      "required": false,
      "type": "boolean",
      "default": "false"
    },
    {
      "name": "ui_framework",
      "description": "UI Framework",
      "required": false,
      "type": "choice",
      "options": ["none", "element-plus", "ant-design-vue", "vuetify"]
    }
  ],
  "dependencies": [],
  "tags": ["vue", "vite", "frontend", "spa"]
}